<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.orderit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>
        <activity android:name=".DinerActivity"></activity>

        <activity android:name=".DinerAdapter"></activity>
        <activity android:name=".FastFoodActivity"></activity>
        <activity android:name=".PizzaActivity"></activity>
        <activity android:name=".SushiActivity"></activity>
        <activity android:name=".TeahouseActivity"></activity>
        <activity android:name=".CoffeesActivity"></activity>
        <activity android:name=".SteakHouseActivity"></activity>






    </application>

</manifest>